class Solution {
public:
    int percentageLetter(string s, char letter) {
        int n=s.size();
        int counts=0;
        for(int i=0;i<n;i++)
        {
            if(s[i]==letter)
            {
                counts++;
            }
        }//for
        int ans=counts*100/n;
        return ans;
        
    }
};
